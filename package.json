    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.3.3",
